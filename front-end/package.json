{
  "name": "pumpkin-fullstack-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "copy-localhost-env": "cp -v envs/.env-localhost .env",
    "copy-dev-env": "cp -v envs/.env-dev .env",
    "copy-staging-env": "cp -v envs/.env-staging .env",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "watch": "webpack serve --mode development --progress",
    "start:local": "yarn copy-localhost-env && yarn --production=false && yarn watch",
    "start:dev": "yarn copy-dev-env && yarn --production=false && yarn watch",
    "start:staging": "yarn copy-staging-env && yarn --production=false && yarn watch",
    "start": "yarn start:local",
    "build": "webpack --mode production --progress"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "axios": "^0.24.0",
    "bootstrap": "^5.1.3",
    "connected-react-router": "^6.8.0",
    "history": "^4.10.1",
    "react": "^17.0.0",
    "react-bootstrap": "^2.0.3",
    "react-bootstrap-table-next": "^4.0.3",
    "react-bootstrap-table2-editor": "^1.4.0",
    "react-dom": "^17.0.0",
    "react-redux": "^7.1.3",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
